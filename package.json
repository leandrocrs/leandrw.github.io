{
  "name": "leandrw.github.io",
  "version": "0.1.0",
  "description": "leandrw static website",
  "main": "README.md",
  "scripts": {
    "build": "npx eleventy",
    "start": "npm run clean && npm run watch:styles & eleventy --serve --port=$npm_package_config_port",
    "clean": "npx rimraf $npm_package_config_path_build",
    "gulp": "npx gulp",
    "build:styles": "npx postcss $npm_package_config_src_css --dir $npm_package_config_dest_css",
    "watch:styles": "npx postcss -w $npm_package_config_src_css --dir $npm_package_config_dest_css"
  },
  "config": {
    "port": 8081,
    "src_css": "styles/**/*.css",
    "dest_css": "_assets/css",
    "path_build": "_site"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leandrw/leandrw.github.io.git"
  },
  "keywords": [
    "leandrw",
    "site",
    "blog",
    "11ty"
  ],
  "private": true,
  "author": "leandrw <contato@leandrw.com.br>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leandrw/leandrw.github.io/issues"
  },
  "homepage": "https://github.com/leandrw/leandrw.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "autoprefixer": "^9.5.0",
    "gulp": "^4.0.0",
    "luxon": "^1.12.0",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "postcss-cli": "^6.1.2",
    "postcss-preset-env": "^6.6.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {}
}
